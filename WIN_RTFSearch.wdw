        type : 14
      -
        internal_properties : CAAAAAgAAAAzBSpRfe4Cxk+cD8qkbwBJI338zEiNk3vfkjOAWAWvv8FsbthD4FE9uPPzBIimbb53mmpgoHQKLPZVQHUIyLZMhfkMcw2kIADcxnpUvkpwAuNRkKvCYy6vuOClPD/Zb1xt/f+JKg9Wenxej9Q=
        code : |1-
         
         // The search parameters are initialized whenever the choice is modified.
         InitializeSearch()
         
         // Hides the default search parameters
         EDT_SoughtWord..Visible = False
         BTN_Color..Visible = False
         
         // According to the selected search mode
         SWITCH MySelf
         	// Search for a word
         	CASE 1
         		// Defines the current operation
         		gsOperationToPerform = <§$0000§>
         		// Information about the operation
         		STC_Info = <§$000c§>
         		// Displays the search edit control
         		EDT_SoughtWord..Visible = True
         		// Input in the control
         		ReturnToCapture(EDT_SoughtWord)
         		
         	// Find the words written in a specific color
         	CASE 2
         		// Defines the current operation
         		gsOperationToPerform = <§$0004§>
         		// Information about the operation
         		STC_Info = <§$0003§>
         		// Displays the button for choosing color
         		BTN_Color..Visible = True
         		// Runs the selection code of color
         		ExecuteProcess(BTN_Color, trtClick)
         		
         	// Find the words written in bold
         	CASE 3
         		// Defines the current operation
         		gsOperationToPerform = <§$0008§>
         		// Information about the operation
         		STC_Info = <§$0007§>  
         		
         	// Search for the words written in italic		
         	CASE 4
         		// Defines the current operation
         		gsOperationToPerform = <§$000a§>
         		// Information about the operation
         		STC_Info = <§$000b§>
         		
         END
         
         // Repositions the cursor at the top
         EDT_RTFControl..Cursor = 0
        type : 17
