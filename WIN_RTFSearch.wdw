        code : |1+
         // Clears the default caption
         MySelf..Caption = ""
         // Default background color
         MySelf..BackgroundColor = LightRed
         // Stores the default search color
         gnColor = MySelf..BackgroundColor
        type : 14
      -
        code : |1-
         // Retrieves the default color
         nCurrentColor is int = MySelf..BackgroundColor
         
         // Opens the window for choosing color
         IF InAutomaticTestMode() = False THEN
         	IF SelectColor(nCurrentColor, scNotPersoColor+scSolidColor) = False THEN RETURN
         END
         
         // Applies the new color
         MySelf..BackgroundColor = nCurrentColor
         
         // Stores the search color
         gnColor = MySelf..BackgroundColor
        type : 18
